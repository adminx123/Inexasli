{
    "name": "my-web-app",
    "version": "1.0.0",
    "description": "A web application project.",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --mode development",
        "build": "webpack --mode production",
        "test": "jest",
        "login": "npx wrangler login",
        "deploy:master": "npx wrangler deploy --config app/wrangler-master.toml --env master",
        "deploy:rate-limiter": "npx wrangler deploy -e rate-limiter",
        
        "deploy:oauth": "npx wrangler deploy --config product/_social/wrangler-oauth.toml",
        "deploy:stripe-app": "npx wrangler deploy --config app/payment/wrangler-stripe-app.toml",
        "deploy:reply-inexasli": "npx wrangler deploy --config products/reply/reply-inexasli.toml",
        "deploy:inexasli": "npx wrangler deploy --config product/client/INEXASLI/hub/hub-inexasli.toml",
        "deploy:meofia": "npx wrangler deploy --config product/_social/clients/social-meofia.toml --env meofia"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "html5-qrcode": "^2.3.8",
        "webpack": "^5.0.0",
        "webpack-cli": "^4.0.0",
        "webpack-dev-server": "^3.0.0"
    },
    "devDependencies": {
        "babel-loader": "^8.0.0",
        "css-loader": "^5.0.0",
        "jest": "^26.0.0",
        "style-loader": "^2.0.0",
        "wrangler": "^4.14.4"
    }
}
